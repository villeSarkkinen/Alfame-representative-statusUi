setup_travis


default_platform(:ios)

platform :ios do

  desc "test"
  lane :test do
    # match(type: "appstore")
    build_app(scheme: "customerrepresentativestatus",
    export_method: "app-store",
    )
  end

  desc "Push a new beta build to TestFlight"
  lane :beta do
 
    # increment_build_number(xcodeproj: "customerrepresentativestatus.xcodeproj")
    match(
      type: "appstore",
      readonly: is_ci
    )
    build_app(scheme: "customerrepresentativestatus",
    export_method: "app-store",
    )
    # upload_to_testflight
  end
end
